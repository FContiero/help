public void comandosLinux() {
	public void principaisComandos() {
		//Baixa aplicativo (Necessário que arquivos para instalação estejam no repositorio)
		sudo apt-get install nomeAplicativo;
		
		//Atualiza repositorio do linux(local)
		sudo apt-get update;
		
		//Adiciona os arquivos de instalação ao repositorio local
		sudo add-apt-repository ppaDoAplicativo;
		
		//Deseinstalar aplicativo
		sudo apt-get remove nomeAplicativo;
	}
	
	//Instalar o git
	public void instalarGit() {
		sudo apt-get update;
		sudo apt-get install git;
	}
	
	//Instala notepad++ do linux
	public void instalarNotepadqq() {
		sudo add-apt-repository ppa:notepadqq-team/notepadqq;
		sudo apt-get update;
		sudo apt-get install notepadqq;
	}
	
	//Instala editor de texto para programação web
	public void instalarAtom() {
		sudo add-apt-repository ppa:webupd8team/atom -y;
		sudo apt-get update;
		sudo apt-get install atom;
	}
	
	//Instala spotify
	public void instalarSpotify() {
		sudo apt-key adv --keyserver hkp://keyserver.ubuntu.com:80 --recv-keys BBEBDCB318AD50EC6865090613B00F1FD2C19886;
		echo deb http://repository.spotify.com stable non-free | sudo tee /etc/apt/sources.list.d/spotify.list;
		sudo apt-get update;
		sudo apt-get install spotify-client;
	}
}
